{
  "expo": {
    "name": "NoGame",
    "slug": "nogameapp",
    "scheme": "app.nogame",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.nogame"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF",
        "versionCode": 100000000
      },
      "package": "app.nogame"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
